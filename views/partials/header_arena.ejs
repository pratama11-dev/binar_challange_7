<header class="border-bottom mb-4 sticky-top">
  <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
    <div class="container">
      <a class="navbar-brand" href="/">Panel Game</a>

      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <% if (url.includes('/arena/login') ||
            url.includes('/arena/create/user') ) { %>

            <a href="/arena/login" class="nav-link" aria-current="page"
              >Login</a
            >
          </li>
          <% } else { %>
          <li class="nav-item">
            <a href="/arena/login" class="nav-link" aria-current="page"
              >Logout</a
            >
          </li>
          <% } %>
        </ul>
      </div>
    </div>
  </nav>
</header>
